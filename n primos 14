def  l100kmtompg ( litros ):
    millas = ( 100 * 1000 ) / 1609.344
    galones =  litros / 3.785411784
    volver  millas / galones

def  mpgtol100km ( millas ):
    litros = 3,785411784
    kim = ( millas * 1609.344 ) / 1000
    km = km / 100 
    retorno  litros / km



